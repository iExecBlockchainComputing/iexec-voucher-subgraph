{
  "name": "iexec-voucher-subgraph",
  "version": "1.0.0",
  "description": "Subgraph for iExec Voucher contracts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iExecBlockchainComputing/iexec-voucher-subgraph.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iExecBlockchainComputing/iexec-voucher-subgraph/issues"
  },
  "homepage": "https://github.com/iExecBlockchainComputing/iexec-voucher-subgraph#readme",
  "main": "index.js",
  "scripts": {
    "schema": "./scripts/generate-schema-and-subgraph.sh",
    "codegen": "cp subgraph.template.yaml subgraph.yaml && graph codegen",
    "build": "graph build",
    "create": "graph create --node ${GRAPHNODE_URL:-http://localhost:8020} bellecour/iexec-voucher",
    "remove": "graph remove --node ${GRAPHNODE_URL:-http://localhost:8020} bellecour/iexec-voucher",
    "deploy:prepare-manifest": "sed -i \"s|#VOUCHER_HUB_ADDRESS#|address: \\\"$VOUCHER_HUB_ADDRESS\\\"|g\" subgraph.yaml && sed -i \"s|#VOUCHER_HUB_START_BLOCK#|startBlock: $VOUCHER_HUB_START_BLOCK|g\" subgraph.yaml",
    "deploy": "graph deploy --node ${GRAPHNODE_URL:-http://localhost:8020} --ipfs ${IPFS_URL:-http://localhost:5001} bellecour/iexec-voucher --version-label ${VERSION_LABEL:-develop}",
    "clean": "rm -rf generated && rm -rf build",
    "all": "npm run clean && npm run codegen && npm run build && npm run create && npm run deploy",
    "test": "graph test",
    "all-local": "./scripts/all-local.sh"
  },
  "author": "",
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.71.1",
    "@graphprotocol/graph-ts": "^0.35.1",
    "@iexec/poco": "^5.3.0"
  },
  "devDependencies": { "matchstick-as": "0.5.0" }
}
