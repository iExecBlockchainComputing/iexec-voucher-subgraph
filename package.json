{
  "name": "iexec-voucher-subgraph",
  "version": "1.0.0",
  "description": "Subgraph for iExec Voucher contracts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iExecBlockchainComputing/iexec-voucher-subgraph.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/iExecBlockchainComputing/iexec-voucher-subgraph/issues"
  },
  "homepage": "https://github.com/iExecBlockchainComputing/iexec-voucher-subgraph#readme",
  "main": "index.js",
  "scripts": {
    "codegen": "./generate_subgraph.sh ${NETWORK_NAME:-c13} subgraph.yaml && graph codegen subgraph.${NETWORK_NAME:-c13}.yaml",
    "build": "graph build subgraph.${NETWORK_NAME:-c13}.yaml",
    "create": "graph create ${NETWORK_NAME:-c13}/iexec-voucher --node ${GRAPHNODE_URL:-http://localhost:8020} ",
    "remove": "graph remove  ${NETWORK_NAME:-c13}/iexec-voucher --node ${GRAPHNODE_URL:-http://localhost:8020}",
    "deploy": "graph deploy ${NETWORK_NAME:-c13}/iexec-voucher subgraph.${NETWORK_NAME:-c13}.yaml --node ${GRAPHNODE_URL:-http://localhost:8020} --ipfs ${IPFS_URL:-http://localhost:5001} --version-label ${VERSION_LABEL:-develop}",
    "clean": "rm -rf generated && rm -rf build",
    "all": "npm run clean && npm run codegen && npm run build && npm run create && npm run deploy",
    "test": "graph test",
    "all-local": "./scripts/all-local.sh"
  },
  "author": "",
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.71.1",
    "@graphprotocol/graph-ts": "^0.35.1",
    "@iexec/poco": "^5.3.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
